                            "noCharging",
                            "charging",
                            "veryFastCharging",
                            "fastCharging",
                            "v2lDischarge",
                            "v2lCharging",
                            "v2lStop",
                            "v2xDischarge",
                            "v2xCharging",
                            "reseveredCharging"
