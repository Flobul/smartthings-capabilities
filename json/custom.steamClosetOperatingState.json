        "steamClosetJobState": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "enum": [
                            "none",
                            "steaming",
                            "airwashing",
                            "sterilizing",
                            "drying",
                            "dehumidifying",
                            "delaywash",
