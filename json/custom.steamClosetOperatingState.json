                    "value"
                ]
            },
            "enumCommands": []
        },
        "completionTime": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "pattern": "^(?:[1-9]\\d{3}-?(?:(?:0[1-9]|1[0-2])-?(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-?(?:29|30)|(?:0[13578]|1[02])-?31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-?02-?29)T(?:[01]\\d|2[0-3]):?[0-5]\\d:?[0-5]\\d(?:\\.\\d{3})?(?:Z|[+-][01]\\d(?::?[0-5]\\d)?)$",
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "value"
                ]
            },
            "enumCommands": []
        },
        "steamClosetMachineState": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "enum": [
                            "pause",
                            "run",
                            "stop"
                        ],
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "value"
                ]
            },
            "enumCommands": []
        },
        "supportedSteamClosetMachineState": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "items": {
                            "enum": [
                                "pause",
                                "run",
                                "stop"
                            ],
                            "type": "string"
                        },
                        "type": "array"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "value"
                ]
            },
            "enumCommands": []
