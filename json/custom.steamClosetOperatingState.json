            "name": "setSteamClosetDelayEndTime",
            "arguments": [
                {
                    "name": "delayEndTime",
                    "optional": false,
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        },
        "setSteamClosetMachineState": {
            "name": "setSteamClosetMachineState",
            "arguments": [
                {
                    "name": "state",
                    "optional": false,
