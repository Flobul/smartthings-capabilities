                    },
                    "paused": {
                        "label": "En pause"
                    },
                    "stopped": {
                        "label": "Arr\u00eat\u00e9"
                    },
                    "standby": {
                        "label": "En veille"
                    },
                    "stop": {
                        "label": "Arr\u00eater"
                    },
                    "playing": {
                        "label": "Lecture"
                    },
                    "pause": {
                        "label": "Pause"
                    }
                }
            }
        }
