{
    "tag": "en",
    "label": "Consumable item",
    "description": "For devices with replaceable components",
    "attributes": {
        "consumableStatus": {
            "label": "Consumable item",
            "description": "The status of replaceable components",
            "displayTemplate": "{{device.label}} {{attribute}} was {{value}}",
            "i18n": {
                "value": {
                    "maintenance_required": {
                        "label": "Maintenance required"
                    },
                    "missing": {
                        "label": "Missing"
                    },
                    "replace": {
                        "label": "Replace"
                    },
                    "good": {
                        "label": "Good"
                    },
                    "order": {
                        "label": "Order"
                    }
                }
            }
        }
    },
    "commands": {
        "setConsumableStatus": {
            "description": "Set the consumable status",
            "arguments": {
                "status": {
                    "i18n": {
                        "missing": {
                            "label": "Missing"
                        },
                        "maintenance required": {
                            "label": "Maintenance required"
                        },
                        "replace": {
                            "label": "Replace"
                        },
                        "good": {
                            "label": "Good"
                        },
                        "order": {
                            "label": "Order"
                        }
                    },
                    "description": "Should be one of ``good``, ``maintenance_required``, ``missing``, ``order``, or ``replace``"
                }
            }
        }
    }
}